	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="ArchotechRib"]/costList</xpath>
		<success>Always</success>
		<value>
			<costList>
				<Uranium>5</Uranium>
				<Gold>10</Gold>
				<Plasteel>10</Plasteel>
