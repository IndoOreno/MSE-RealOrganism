<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Prosthetic Rib -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="SimpleProstheticRib"]/costList</xpath>
		<success>Always</success>
		<value>
			<costList>
				<Steel>10</Steel>
			</costList>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="SimpleProstheticRib"]/statBases</xpath>
		<success>Always</success>
		<value>
			<statBases>
				<MarketValue>25</MarketValue>
				<WorkToMake>1200</WorkToMake>
				<Mass>0.3</Mass>
			</statBases>
		</value>
	</Operation>

</Patch>